    <resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="ALLBLACK" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:windowTranslucentStatus">true</item>
        </style>


    <style name="Transparancy" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowTranslucentStatus">true</item>
    </style>


        <style name="MYNAVBAR" parent="ThemeOverlay.AppCompat.Dark">
            <item name="colorControlActivated">@color/main</item>
            <item name="colorControlHighlight">@color/One</item>
            <item name="colorPrimary">@color/main</item>
            <item name="colorAccent">@color/colorAccent</item>

        </style>






    </resources>
