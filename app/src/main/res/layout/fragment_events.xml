<?xml version="1.0" encoding="utf-8"?>
<com.sothree.slidinguppanel.SlidingUpPanelLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sothree="http://schemas.android.com/apk/res-auto"
    android:id="@+id/sliding_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom"
    sothree:umanoPanelHeight="0dp"
    sothree:umanoDragView="@+id/dragView"
    sothree:umanoOverlay="true"
    sothree:umanoScrollableView="@+id/list">

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/Backdrop">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:orientation="horizontal"
        android:layout_weight="1" >


        <LinearLayout
            android:id="@+id/cini"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#0e0e0e"
            android:clickable="true"
            android:foreground="?selectableItemBackground"
            android:gravity="center_horizontal|center_vertical"
            android:orientation="vertical">

            <ImageViewfragment
                android:id="@+id/ONE"
                android:layout_width="70dip"
                android:layout_height="70dip"
                android:layout_marginBottom="55dip"
                android:alpha="0.5" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-55dip"
                android:alpha="0.5"
                android:fontFamily="sans-serif-light"
                android:paddingEnd="19dip"
                android:paddingStart="19dip"
                android:text="CINEFELIA"
                android:textAllCaps="true"
                android:textColor="#ffffff"
                android:textSize="20sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/shabd"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#121212"
            android:clickable="true"
            android:foreground="?selectableItemBackground"
            android:gravity="center_horizontal|center_vertical"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/TWO"
                android:layout_width="70dip"
                android:layout_height="70dip"
                android:layout_marginBottom="55dip"
                android:alpha="0.5" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-55dip"
                android:alpha="0.5"
                android:fontFamily="sans-serif-light"
                android:paddingEnd="19dip"
                android:paddingStart="19dip"
                android:text="SHABD"
                android:textAllCaps="true"
                android:textColor="#ffffff"
                android:textSize="20sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:orientation="horizontal"
        android:layout_weight="1" >

        <LinearLayout
            android:id="@+id/Litmus"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#121212"
            android:clickable="true"
            android:foreground="?selectableItemBackground"
            android:gravity="center_horizontal|center_vertical"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/THREE"
                android:layout_width="70dip"
                android:layout_height="70dip"
                android:layout_marginBottom="55dip"
                android:alpha="0.5" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-55dip"
                android:alpha="0.5"
                android:fontFamily="sans-serif-light"
                android:paddingEnd="19dip"
                android:paddingStart="19dip"
                android:text="LITMUS"
                android:textAllCaps="true"
                android:textColor="#ffffff"
                android:textSize="20sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/TMC"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#0e0e0e"
            android:clickable="true"
            android:foreground="?selectableItemBackground"
            android:gravity="center_horizontal|center_vertical"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/FOUR"
                android:layout_width="70dip"
                android:layout_height="70dip"
                android:layout_marginBottom="55dip"
                android:alpha="0.5" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-55dip"
                android:alpha="0.5"
                android:fontFamily="sans-serif-light"
                android:paddingEnd="19dip"
                android:paddingStart="19dip"
                android:text="TMC"
                android:textAllCaps="true"
                android:textColor="#ffffff"
                android:textSize="20sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:orientation="horizontal"
        android:layout_weight="1" >

        <LinearLayout
            android:id="@+id/scribbles"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#0e0e0e"
            android:clickable="true"
            android:foreground="?selectableItemBackground"
            android:gravity="center_horizontal|center_vertical"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/FIVE"
                android:layout_width="70dip"
                android:layout_height="70dip"
                android:layout_marginBottom="55dip"
                android:alpha="0.5" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-55dip"
                android:alpha="0.5"
                android:fontFamily="sans-serif-light"
                android:paddingEnd="19dip"
                android:paddingStart="19dip"
                android:text="SCRIBBLES"
                android:textAllCaps="true"
                android:textColor="#ffffff"
                android:textSize="20sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/SOPHIA"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#121212"
            android:clickable="true"
            android:foreground="?selectableItemBackground"
            android:gravity="center_horizontal|center_vertical"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/SIX"
                android:layout_width="70dip"
                android:layout_height="70dip"
                android:layout_marginBottom="55dip"
                android:alpha="0.5" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-55dip"
                android:alpha="0.5"
                android:fontFamily="sans-serif-light"
                android:paddingEnd="19dip"
                android:paddingStart="19dip"
                android:text="SOPHIA"
                android:textAllCaps="true"
                android:textColor="#ffffff"
                android:textSize="20sp" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:orientation="horizontal"
        android:layout_weight="1" >

        <LinearLayout
            android:id="@+id/Aperture"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#121212"
            android:clickable="true"
            android:foreground="?selectableItemBackground"
            android:gravity="center_horizontal|center_vertical"
            android:orientation="vertical">



            <ImageView
                android:id="@+id/SEVEN"
                android:layout_width="70dip"
                android:layout_height="70dip"
                android:alpha="0.5"
                android:layout_marginBottom="55dip"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-55dip"
                android:alpha="0.5"
                android:fontFamily="sans-serif-light"
                android:paddingEnd="19dip"
                android:paddingStart="19dip"
                android:text="APERTURE"
                android:textAllCaps="true"
                android:textColor="#ffffff"
                android:textSize="20sp" />

        </LinearLayout>




        <LinearLayout
            android:id="@+id/coreoe"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#0e0e0e"
            android:clickable="true"
            android:foreground="?selectableItemBackground"
            android:gravity="center_horizontal|center_vertical"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/EIGHT"
                android:layout_width="70dip"
                android:layout_height="70dip"
                android:layout_marginBottom="55dip"
                android:alpha="0.5" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-55dip"
                android:alpha="0.5"
                android:fontFamily="sans-serif-light"
                android:paddingEnd="19dip"
                android:paddingStart="19dip"
                android:text="COREOGRAFIA"
                android:textAllCaps="true"
                android:textColor="#ffffff"
                android:textSize="20sp" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>


    <FrameLayout
        android:clickable="true"
        android:focusable="true"
        android:background="#ea151515"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <View
            android:layout_width="1dip"
            android:layout_height="match_parent"
            android:background="#ff999999"
            android:layout_marginLeft="32dp"/>
        <android.support.v7.widget.RecyclerView
            android:id="@+id/RECYCLE"
            android:nestedScrollingEnabled="true"
            android:layout_marginBottom="15dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </FrameLayout>
</com.sothree.slidinguppanel.SlidingUpPanelLayout>